# MongoDB Connection String
# Get this from MongoDB Atlas dashboard
MONGO_DB_URL=mongodb+srv://username:password@cluster.mongodb.net/todo-app?retryWrites=true&w=majority

# JWT Secret Token
# Generate a random string for this
TOKEN=your_super_secret_jwt_token_here

# Server Port (optional, defaults to 5000)
PORT=5000

# Node Environment
NODE_ENV=development
